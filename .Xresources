! special
*foreground      : #f8f8f2
*foreground_bold : #f8f8f2
*cursor          : #90A4AE
*background      : #121212

! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color10:  #98971a
*color2: #b8bb26
! DarkYellow + Yellow
*color11:  #d79921
*color3: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color13:  #b16286
*color5: #d3869b
! DarkCyan + Cyan
*color14:  #689d6a
*color6: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

! Rofi
rofi.color-enabled: true
rofi.color-window: #242424, #c6c8c5, #c6c8c5
rofi.color-normal: #242424, #707880, #242424, #242424, #c6c8c5
rofi.color-active: #242424, #707880, #242424, #242424, #c6c8c5
rofi.color-urgent: #242424, #707880, #242424, #242424, #c6c8c5

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: Source Code Pro 9
rofi.bw: 1
rofi.columns: 3
rofi.padding: 4
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true

